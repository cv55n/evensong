{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@discordjs/guide",
  "version": "0.1.0",
  "description": "imagine um bot... o jeito mais popular de construir bots de discord",
  "private": true,
  "scripts": {
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "build:check": "tsc --noEmit",
    "build:local": "cross-env NEXT_PUBLIC_LOCAL_DEV=true pnpm run build:prod",
    "build:prod": "pnpm run build:next",
    "build:next": "next build",
    "build": "next build --webpack",
    "preview": "next start",
    "preview:cf": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy:cf": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "dev": "next dev -p 3001 --turbopack",
    "lint": "pnpm run build:check && prettier --check . && cross-env TIMING=1 eslint --format=pretty src ",
    "format": "pnpm run build:check && prettier --write . && cross-env TIMING=1 eslint --fix --format=pretty src ",
    "fmt": "pnpm run format",
    "postinstall": "next typegen && fumadocs-mdx"
  },
  "type": "module",
  "directories": {
    "lib": "src"
  },
  "license": "Apache-2.0",
  "keywords": [
    "discord",
    "api",
    "bot",
    "client",
    "node",
    "discordapp",
    "discordjs"
  ],
  "dependencies": {
    "@opennextjs/cloudflare": "^1.13.1",
    "@react-icons/all-files": "^4.1.0",
    "@vercel/analytics": "^1.5.0",
    "cmdk": "^1.1.1",
    "cva": "^1.0.0-beta.3",
    "fumadocs-core": "^16.1.0",
    "fumadocs-mdx": "^14.0.3",
    "fumadocs-twoslash": "^3.1.10",
    "fumadocs-ui": "^16.1.0",
    "geist": "^1.5.1",
    "immer": "^11.0.0",
    "jotai": "^2.15.1",
    "jotai-immer": "^0.4.1",
    "lucide-react": "^0.555.0",
    "mermaid": "^11.12.1",
    "motion": "^12.23.24",
    "next": "^16.0.4",
    "next-mdx-remote-client": "^2.1.7",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.1",
    "p-retry": "^7.1.0",
    "react": "^19.2.0",
    "react-aria": "^3.44.0",
    "react-aria-components": "^1.13.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "safe-mdx": "^1.3.9",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "twoslash": "^0.3.4",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@next/env": "^16.0.4",
    "@shikijs/rehype": "^3.15.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "babel-plugin-react-compiler": "^1.0.0",
    "cpy-cli": "^6.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-neon": "^0.2.9",
    "eslint-formatter-pretty": "^7.0.0",
    "git-describe": "^4.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "remark-gfm": "^4.0.1",
    "remark-rehype": "^11.1.2",
    "shiki": "^3.15.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-react-aria-components": "^2.0.1",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "vercel": "^48.11.0",
    "wrangler": "^4.51.0"
  },
  "engines": {
    "node": ">=22.12.0"
  }
}
