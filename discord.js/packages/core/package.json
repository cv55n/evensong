{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@discordjs/core",
  "version": "2.1.0",
  "description": "uma camada de abstração leve que envolve a api rest e o gateway.",
  "scripts": {
    "test": "vitest run --config ../../vitest.config.ts",
    "build": "tsc --noEmit && tsup",
    "build:docs": "tsc -p tsconfig.docs.json",
    "lint": "prettier --check . && cross-env TIMING=1 eslint --format=pretty src",
    "format": "prettier --write . && cross-env TIMING=1 eslint --fix --format=pretty src",
    "docs": "pnpm run build:docs && api-extractor run --local --minify && generate-split-documentation",
    "prepack": "pnpm run build && pnpm run limit",
    "changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/core/*'",
    "release": "cliff-jumper"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./http-only": {
      "require": {
        "types": "./dist/http-only.d.ts",
        "default": "./dist/http-only.js"
      },
      "import": {
        "types": "./dist/http-only.d.mts",
        "default": "./dist/http-only.mjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "keywords": [],
  "dependencies": {
    "@sapphire/snowflake": "^3.5.5",
    "@vladfrangu/async_event_emitter": "^2.4.7",
    "discord-api-types": "^0.38.34"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^6.0.0",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.14",
    "cross-env": "^10.1.0",
    "esbuild-plugin-version-injector": "^1.2.1",
    "eslint": "^9.39.1",
    "eslint-config-neon": "^0.2.9",
    "eslint-formatter-compact": "^9.0.1",
    "eslint-formatter-pretty": "^7.0.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
